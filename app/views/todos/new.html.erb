<h2 align="center">New Todo</h2>


<div align="center">
<%= render 'todoerrors' %>

	<%= form_for @todo do |f| %>
		<p>
			<%= f.label :name %>
			<%= f.text_field :name %>
		</p>

		<p>
			<%= f.label :description %>
			<%= f.text_area :description %>
		</p>

		<p>
			<%= f.submit %>
		</p>

	<% end %>

</div>